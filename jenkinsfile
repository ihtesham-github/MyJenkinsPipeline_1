pipeline {
  agent none
    stages {
        stage ('Build')
        {
        steps{
        echo "building project"
        }
        }
        stage ('Test')
        {
        steps{
        echo "testing project"
        }
        }
      stage ('Deploy')
        {
        steps{
        echo "deploying project"
        }
        }
stage ('Release')
      {
      steps{
      echo "releasing project"
      }
      }
      }   
      }
